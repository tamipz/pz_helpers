
alias aws:login="aws sso login"

alias docker:login="aws ecr get-login-password --region us-east-1 | docker login --username AWS --password-stdin 936143655872.dkr.ecr.us-east-1.amazonaws.com"

alias login:all="aws:login && docker:login"

export AWS_ECR_REGISTRY=936143655872.dkr.ecr.us-east-1.amazonaws.com
export AWS_PROFILE=default
export AWS_CDN_BUCKET="staging.static.payzen.com"


